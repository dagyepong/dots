
*.font: xft:Hack Nerd Font:style=Regular:size=12


! special
*.foreground:   #f0f0f0
*.background:   #0f0f0f
*.cursorColor:  #f0f0f0

! black
*.color0:       #262626
*.color8:       #4c4c4c

! red
*.color1:       #ac8a8c
*.color9:       #c49ea0

! green
